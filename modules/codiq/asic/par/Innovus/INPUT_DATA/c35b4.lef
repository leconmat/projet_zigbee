# Via Array for Wide-Met1/Wide-Met2
VIARULE VIA1W2W_G GENERATE
    LAYER MET2 ;
       DIRECTION VERTICAL ; 
       WIDTH 10 TO 1000.0 ; 
       OVERHANG 0.2 ;
       METALOVERHANG 0.00 ;

    LAYER MET1 ;
        DIRECTION HORIZONTAL ; 
        WIDTH 10 TO 1000.0 ; 
        OVERHANG 0.2 ;
        METALOVERHANG 0.00 ;

    LAYER VIA1 ;
        RECT -0.250000 -0.250000 0.250000 0.250000 ;
        SPACING 0.450000 BY 0.450000 ;
    RESISTANCE 3 ;
END VIA1W2W_G

