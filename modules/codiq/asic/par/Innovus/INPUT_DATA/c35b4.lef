# Via Met1-Met2 for normal routing
VIA VIA1_PR DEFAULT
    RESISTANCE 3 ;
    FOREIGN VIA1_C ;
    LAYER MET1 ;
        RECT -0.450000 -0.450000 0.450000 0.450000 ;
    LAYER VIA1 ;
        RECT -0.250000 -0.250000 0.250000 0.250000 ;
    LAYER MET2 ;
        RECT -0.450000 -0.450000 0.450000 0.450000 ;
END VIA1_PR

